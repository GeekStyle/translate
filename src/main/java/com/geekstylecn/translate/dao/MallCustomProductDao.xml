<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 
<mapper namespace="com.geekstylecn.translate.dao.MallCustomProductDao">

	<select id="getMallCustomProduct" parameterType="long" resultType="MallCustomProduct">
		select
			*
		from
			mall_custom_product
		where 
			id = #{id}
	</select>
	
	<select id="getAllMallCustomProduct" resultType="MallCustomProduct">
		select
			*
		from
			mall_custom_product
	</select>
	
</mapper>